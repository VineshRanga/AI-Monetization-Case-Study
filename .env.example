# Format: "Your Name your.email@domain.com"
# Example: "John Doe john.doe@example.com"
SEC_USER_AGENT=Your Name your.email@domain.com

# FRED API (Optional - can use CSV fallback)
FRED_API_KEY=

# Logging
LOG_LEVEL=INFO
